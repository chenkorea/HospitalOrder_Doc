<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <include layout="@layout/loading_view_952658" />
    <!--
    <ImageView
        android:id="@+id/iv_diet_head"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:contentDescription="咨询头像"
        android:gravity="center_vertical"
        android:src="@drawable/head_default_yixin" />-->

    <com.boil.hospitalorder.utils.PtrClassicFrameLayout
        xmlns:cube_ptr="http://schemas.android.com/apk/res-auto"
        android:id="@+id/rotate_header_list_view_frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/cube_mints_f1f1f1"
        cube_ptr:ptr_duration_to_close="200"
        cube_ptr:ptr_duration_to_close_header="1000"
        cube_ptr:ptr_keep_header_when_refresh="true"
        cube_ptr:ptr_pull_to_fresh="false"
        cube_ptr:ptr_ratio_of_header_height_to_refresh="1.2"
        cube_ptr:ptr_resistance="1.7" >

        
        
        <com.boil.hospitalorder.utils.CTListView
            android:id="@+id/listview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:choiceMode="singleChoice"
            android:divider="@color/listview_divider"
            android:dividerHeight="0.5dp"
            android:scrollbarStyle="outsideOverlay" >
            
        </com.boil.hospitalorder.utils.CTListView>
        
        
    </com.boil.hospitalorder.utils.PtrClassicFrameLayout>

</LinearLayout>
